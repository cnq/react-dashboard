{
  "name": "universal-dashboard",
  "version": "1.0.0",
  "description": "A starter dashboard that can be used on any project.",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "webpack-dev-server --port 8080 --hot --host 0.0.0.0",
    "start:paperhook": "webpack-dev-server --port 8080 --hot --host 0.0.0.0",
    "production": "webpack -p",
    "lint": "npm-run-all --parallel start lint:watch; exit 0",
    "lint:watch": "npm run lint:files -- --watch",
    "lint:files": "node_modules/.bin/esw webpack.config.* tools app/.",
    "fix": "eslint --fix app/.; exit 0"
  },
  "keywords": [
    "dashboard",
    "saas dashboard",
    "react dashboard"
  ],
  "author": "James Ryan Cooper",
  "license": "MIT",
  "dependencies": {
    "firebase": "^3.0.1",
    "immutable": "3.8.1",
    "lodash": "4.13.1",
    "mailcheck": "1.1.1",
    "material-ui": "0.15.1",
    "react": "15.1.0",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.1",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.5.2",
    "redux-form": "^5.2.3",
    "redux-thunk": "2.0.1",
    "validator": "5.2.0"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "axios": "^0.13.1",
    "babel-cli": "6.9.0",
    "babel-core": "6.7.7",
    "babel-eslint": "6.0.0-beta.6",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "colors": "1.1.2",
    "css-loader": "0.23.1",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "9.0.1",
    "eslint-config-standard": "5.3.0",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-jsx-a11y": "1.3.0",
    "eslint-plugin-promise": "1.1.0",
    "eslint-plugin-react": "5.0.1",
    "eslint-plugin-standard": "1.3.2",
    "eslint-watch": "2.1.11",
    "html-webpack-plugin": "2.16.1",
    "npm-run-all": "2.1.1",
    "open": "0.0.5",
    "postcss": "5.0.21",
    "postcss-loader": "0.9.1",
    "style-loader": "0.13.1",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
